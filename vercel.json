{
    "regions": [
        "sin1"
    ],
    "rewrites": [
        {
            "source": "/auth/:path*",
            "destination": "/api/node"
        },
        {
            "source": "/requests/:path*",
            "destination": "/api/node"
        },
        {
            "source": "/api-keys/:path*",
            "destination": "/api/node"
        },
        {
            "source": "/users/:path*",
            "destination": "/api/node"
        },
        {
            "source": "/((?!auth|requests|api-keys|users).*)",
            "destination": "/api/index"
        }
    ]
}