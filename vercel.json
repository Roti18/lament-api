{
    "regions": [
        "sin1"
    ],
    "functions": {
        "api/node.ts": {
            "maxDuration": 10
        },
        "api/index.ts": {
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/api/auth/login",
            "destination": "/api/node"
        },
        {
            "source": "/api/auth/register",
            "destination": "/api/node"
        },
        {
            "source": "/api/:path*",
            "destination": "/api/index"
        },
        {
            "source": "/auth/login",
            "destination": "/api/node"
        },
        {
            "source": "/auth/register",
            "destination": "/api/node"
        },
        {
            "source": "/api-keys/:path*",
            "destination": "/api/node"
        },
        {
            "source": "/:path*",
            "destination": "/api/index"
        }
    ]
}