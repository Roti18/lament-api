{
    "regions": [
        "sin1"
    ],
    "functions": {
        "api/node.ts": {
            "maxDuration": 10
        },
        "api/index.ts": {
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/auth/:path*",
            "destination": "/api/node"
        },
        {
            "source": "/requests/:path*",
            "destination": "/api/node"
        },
        {
            "source": "/api-keys/:path*",
            "destination": "/api/node"
        },
        {
            "source": "/debug-db",
            "destination": "/api/node"
        },
        {
            "source": "/((?!auth|requests|api-keys|debug-db).*)",
            "destination": "/api/index"
        }
    ]
}